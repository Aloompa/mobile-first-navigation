{
  "presets": [
    "babel-preset-expo"
  ],
  "env": {
    "development": {
      "plugins": [
        "@babel/plugin-transform-react-jsx-source"
      ]
    }
  },
  "plugins": [
    "babel-plugin-styled-components",
    [
      "module-resolver",
      {
        "alias": {
          "styled-components/native": "styled-components"
        }
      }
    ]
  ]
}