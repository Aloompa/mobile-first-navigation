{
  "presets": [
    "babel-preset-expo"
  ],
  "env": {
    "development": {
      "plugins": [
        "transform-react-jsx-source"
      ]
    }
  },
  "plugins": [
    "babel-plugin-styled-components",
    [
      "module-resolver",
      {
        "alias": {
          "^react-native$": "react-native-web",
          "styled-components/native": "styled-components"
        }
      }
    ]
  ]
}